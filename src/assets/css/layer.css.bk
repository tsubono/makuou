/* regist
------------------------------------------------------------ */
.regist h1 img { width: 100%; max-width: 192px; }
.form_template .form__bd{
	border: dashed 2px #ccc;
	border-bottom: none;
	margin: 20px 0;
}
.form_template dl{
	display: table;
	width: 100%;
	border-bottom: dashed 2px #ccc;
}
.form_template dt{
	padding: 35px;
	display: table-cell;
	vertical-align: middle;
	color: #ff5925;
	font-weight: bold;
	width: 30%;
	background: url(../img/common/bg_formhead.gif) repeat;
	border-right: dashed 2px #ccc;
}
.form_template dt span{
	background: #f93211;
	border-radius: 3px;
	color: #fff;
	padding: 10px;
	margin-right: 10px;
}
.form_template dd{ padding: 35px; display: table-cell; }
.innerlist_name { list-style-type: none; }
.innerlist_name li {
	display: table;
	float: right;
	width: 47%;
}
.innerlist_name li:first-child { float: left; }
.innerlist_name li::before {
	content: "名：";
	display: table-cell;
	text-align: right;
	width: 2em;
}
.innerlist_name li:first-child::before { content: "姓："; }
.innerlist_name li input { display: table-cell; }

/* ■カナ */
.innerlist_kana::after {
	clear: both;
	content: "";
	display: block;
}
.innerlist_kana { display: inline-block; }
.innerlist_kana { display: block; }
.innerlist_kana { list-style-type: none; }
.innerlist_kana li {
	display: table;
	float: right;
	width: 47%;
}
.innerlist_kana li:first-child { float: left; }
.innerlist_kana li::before {
	content: "めい：";
	display: table-cell;
	width: 4em;
}
.innerlist_kana li:first-child::before { content: "せい："; }
.innerlist_kana li input { display: table-cell; }

.form_template input[type="text"]{
	width: 100%;
	height: 3em;
	border: solid 1px #ccc;
}
.innar_list li{
	display: inline-block;
	float: left;
	width: 48%;
}
.innar_list li:nth-child(2n){ float: right; }
.innar_list li input[type="text"]{ width: 80%; margin-left: 2%; }
.innar_list2 li{
	display: inline-block;
	float: left;
	width: 30%;
}
.innar_list2 li:nth-child(3n){ margin-right: 0; }
.innar_list2 li input[type="text"]{ width: 80%; margin-right: 7%; }
.form_template input[type="submit"]{
	width: 100%;
	border: none;
	border-bottom: solid 2px #ca5a2d;
	border-radius: 3px;
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#fc6c35+0,f7280d+46,f7240b+47,f30a01+50,fb5b2a+100 */
	background: rgb(252,108,53); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(252,108,53,1) 0%, rgba(247,40,13,1) 46%, rgba(247,36,11,1) 47%, rgba(243,10,1,1) 50%, rgba(251,91,42,1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fc6c35', endColorstr='#fb5b2a',GradientType=0 ); /* IE6-9 */
	color: #fff;
	padding: 15px;
	font-size: 120%;
}
.form_template .sendarea li{ max-width: 200px; margin: 30px auto; }
.form_template dd input[type="text"],
.form_template dd input[type="textarea"],
.form_template dd select,
.form_template dd textarea {
	width: 96%;
	max-width: 100%;
	font-family: inherit;
	padding: 7px 12px;
	font-size: 14px;
	background: #fff;
	border-radius: 4px;
	border: 1px solid #ccc;
}
.form_template dd select{
	width: 30%;
	max-width: 100%;
	font-family: inherit;
	padding: 7px 12px;
	font-size: 14px;
	background: #fff;
	border-radius: 4px;
	border: 1px solid #ccc;
}
.form_template dd .innerlist_tel li input[type="text"]{ width: 22%; }
.form_template dd input[type="radio"],
.form_template dd input[type="checkbox"]{
	box-shadow: none;
	border: 5px solid #ccc;
}
.form_template dd .nest { padding-left: 45px; margin: 10px 0;}
.form_template dd input[type="textarea"] {
	width: 100%;
	height: 200px;
	box-sizing: border-box;
	resize: vertical;
}
.form_template .address_num dd input[type="text"]{ width: 22%; }
.form_template .address_num2 dd select{ width: 30%; }

/* thanks
------------------------------------------------------------ */
.thanks__info{
	background: #fdffce;
	border-radius: 20px;
	display: table;
	width: 100%;
	margin: 30px 0;
}
.thanks__info .pho{
	display: table-cell;
	width: 30%;
	padding: 40px 20px;
}
.thanks__info .pho img{ width: 100%; }
.thanks__info .txt{
	display: table-cell;
	vertical-align: middle;
	padding: 40px 20px;
}
.thanks__info .txt h2{
	color: #ff5b26;
	font-size: 160%;
	font-weight: bold;
	margin-bottom: 20px;
}
.thanks__info .btn{
	max-width: 300px;
	border: none;
	border-bottom: solid 2px #ca5a2d;
	border-radius: 3px;
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#fc6c35+0,f7280d+46,f7240b+47,f30a01+50,fb5b2a+100 */
	background: rgb(252,108,53); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(252,108,53,1) 0%, rgba(247,40,13,1) 46%, rgba(247,36,11,1) 47%, rgba(243,10,1,1) 50%, rgba(251,91,42,1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fc6c35', endColorstr='#fb5b2a',GradientType=0 ); /* IE6-9 */
	margin-top: 20px;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	-o-transition: all .3s;
	transition: all .3s;
}
.thanks__info .btn a{
	color: #fff;
	padding: 10px 15px;
	text-decoration: none;
	text-align: center;
	display: block;
}
.thanks__info .btn:hover{ opacity: 0.8; }

