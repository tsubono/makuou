/* common
------------------------------------------------------------ */

.ttl01{
	background: url("../img/common/ttl_single_01.gif") repeat left center;
	font-size: 24px;
	font-weight: bold;
	color: #fff;
	text-shadow: 0px 0px 5px rgba(0, 0, 0, 0.5);
	padding: 15px;
	margin: 40px 0 20px;
}
.ttl02{
	background: url("../img/common/ttl_single_02.gif") repeat left center;
	font-size: 20px;
	font-weight: bold;
	color: #fff;
	padding: 15px;
	margin: 30px 0 25px;
}
.ttl03{
	background: url(../img/common/arrow_r_pp.png) no-repeat left 10px center / 15px auto,
		url("../img/common/bg_formhead.gif") repeat left center / 6px auto;
	font-size: 17px;
	font-weight: bold;
	border: dashed 2px #ccc;
	color: #ff5925;
	padding: 15px;
	padding-left: 35px;
	margin: 30px 0 20px;
}
.list_dot li{
	background: url(../img/common/list_style.png) no-repeat left center / 12px auto;
	padding-left: 18px;
	list-style: none;
}
.table01 {
	border-left: 1px solid #cccccc;
	border-top: 1px solid #cccccc;
	width: 100%;
	margin: 30px 0;
}
.table01 th {
	background: url(../img/common/ttl_single_04.gif) repeat left center;
	font-weight: bold;
	text-align: center;
	padding: 10px;
}
.table01 th, .table01 td {
	border-right: 1px solid #cccccc;
	border-bottom: 1px solid #cccccc;
	padding: 10px;
}
.table01 td {
    font-size: .875em;
}
.various h6, .various p {
	font-size: 14px;
	line-height: 54px;
	padding-left: 19px;
	padding-right: 19px;
}
.various h6 {
	background: #ff7800;
	color: #fff;
	margin-bottom: 0px;
}
.various .evennumber { font-size: 14px; background: rgba(35, 40, 255, 0.1); }

.leftBox {
	float: left;
	width: 65%;
	display: inline-block;
}
.rightBox {
	float: right;
	width: 30%;
	display: inline-block;
}
.rightBox.pho img,
.leftBox.pho img{ width: 100%; height: auto; }


p.txt {
    font-size: .875rem;
    line-height: 1.8;
}
.l-inner > .pickup {
    width: 100%;
    margin: 40px auto 0;
    padding: 30px 30px 50px;
    box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
    background-color: #fff;
}
.pickup .btn { width: 100%; }
.pickup .btn img { 
    margin: 50px auto 0;
    width: 100%;
    max-width: 606px;
}

/* login
------------------------------------------------------------ */
.login h1 img {  width: 100%; max-width: 189px; }
.login__column{ margin: 30px 0;}
.login__box{
	display: inline-block;
	width: 49%;
	border-radius: 10px;
}
.login__box.member{ float: left; }
.login__box.first{ float: right; }
.login__box h2{
	color: #fff;
	font-size: 24px;
	font-weight: bold;
	text-align: center;
	padding: 20px 10px 20px;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}
.login__box.member h2{
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#f9ad43+0,ff7800+70 */
	background: rgb(249,173,67); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(249,173,67,1) 0%, rgba(255,120,0,1) 70%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, rgba(249,173,67,1) 0%,rgba(255,120,0,1) 70%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, rgba(249,173,67,1) 0%,rgba(255,120,0,1) 70%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9ad43', endColorstr='#ff7800',GradientType=0 ); /* IE6-9 */
}
.login__box.first h2{
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#dcdcfc+0,bdbdfc+70 */
	background: rgb(220,220,252); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(220,220,252,1) 0%, rgba(189,189,252,1) 70%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, rgba(220,220,252,1) 0%,rgba(189,189,252,1) 70%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, rgba(220,220,252,1) 0%,rgba(189,189,252,1) 70%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dcdcfc', endColorstr='#bdbdfc',GradientType=0 ); /* IE6-9 */
}
.login__box .innar{
	background: #fff;
	border: solid 10px;
	border-top: none;
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
	padding: 30px 40px;
}
.login__box.member .innar{ border-color: #ff7800; }
.login__box.first .innar{ border-color: #bdbdfc; text-align: center; }
.login__box .innar .btn{
	border-top: solid 2px #ca5a2d;
	border-radius: 3px;
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#fc6c35+0,f7280d+46,f7240b+47,f30a01+50,fb5b2a+100 */
	background: rgb(252,108,53); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(252,108,53,1) 0%, rgba(247,40,13,1) 46%, rgba(247,36,11,1) 47%, rgba(243,10,1,1) 50%, rgba(251,91,42,1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fc6c35', endColorstr='#fb5b2a',GradientType=0 ); /* IE6-9 */
	max-width: 280px;
	margin: 20px auto 0;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	-o-transition: all .3s;
	transition: all .3s;
}
.login__box .innar .btn:hover{
	opacity: 0.7;
	filter: alpha(opacity=70);
	-ms-filter: "alpha(opacity=70)";
}
.login__box.first p.tec{ margin: 40px 0; }
.login__box .innar .btn a{
	color: #fff;
	display: block;
	padding: 10px;
	text-align: center;
	text-decoration: none;
}
.forget{
	float: right;
	position: relative;
	padding-left: 20px;
}
.forget a{ text-decoration: none; color: #000; }
.forget a:hover{ text-decoration: underline; }
.forget::before{
	background: url(../img/common/arrow_r_red.gif) no-repeat 0 0 / 14px auto;
	content: "";
	position: absolute;
	width: 14px;
	height: 18px;
	left: 0;
	top: 4px;
}
.login__box dt{ margin-bottom: 10px; }
.login__box input[type="text"]{ width: 100%; height: 3em; }
.login__box input[type="submit"]{
	width: 100%;
	border: none;
	border-top: solid 2px #ca5a2d;
	border-radius: 3px;
	/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#fc6c35+0,f7280d+46,f7240b+47,f30a01+50,fb5b2a+100 */
	background: rgb(252,108,53); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(252,108,53,1) 0%, rgba(247,40,13,1) 46%, rgba(247,36,11,1) 47%, rgba(243,10,1,1) 50%, rgba(251,91,42,1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom, rgba(252,108,53,1) 0%,rgba(247,40,13,1) 46%,rgba(247,36,11,1) 47%,rgba(243,10,1,1) 50%,rgba(251,91,42,1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fc6c35', endColorstr='#fb5b2a',GradientType=0 ); /* IE6-9 */
	color: #fff;
	padding: 10px;
	font-size: 100%;
}
.login__box .login_btn{ max-width: 200px; margin: 30px auto; }

