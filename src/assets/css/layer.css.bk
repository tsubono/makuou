.scroll {
    width: 100%;
    overflow-y: hidden;
    overflow-x: auto;
    -ms-overflow-style: -ms-autohiding-scrollbar;
    -webkit-overflow-scrolling: touch;
}
.scroll table {
    width: 100%;
    border-left: 1px solid #cccccc;
    border-top: 1px solid #cccccc;
    border-collapse: collapse;
}
.scroll table th {
    background: url(../img/common/ttl_single_04.png) no-repeat left center;
    font-weight: bold;
    text-align: center;
}
.scroll table th, .scroll table td {
    padding: 8px;
    border-right: 1px solid #cccccc;
    border-bottom: 1px solid #cccccc;
    white-space: nowrap;
}



@media screen and (min-width: 768px) and (max-width: 1119px) {
    
    /* common
	------------------------------------------------------------ */	
	.l-inner > .pickup { 
        width: 96%;
    }
	/* regist
	------------------------------------------------------------ */	
	.form_template dt{ width: 40%; }
	
}
@media screen and (max-width: 767px) {

	/* common
	------------------------------------------------------------ */
	.leftBox {
		float: none;
		width: auto;
		display: block;
	}
	.rightBox {
		float: none;
		width: auto;
		display: block;
	}
	.rightBox.pho,
	.leftBox.pho { width: 80%; margin: 20px auto; }
    
    .table01.tate th { text-align: center; }
    .table01.tate th,.table01.tate td {
        display: block;
        width: 100%;
    }
	
	/* login
	------------------------------------------------------------ */
	.login__box{ display: block; width: auto; }
	.login__box.member{ float: none; margin-bottom: 20px; }
	.login__box.first{ float: none; }
	.login__box h2{ font-size: 16px; padding: 15px 10px 15px; }
	.login__box .innar{ border: solid 5px; padding: 20px; }
	.login__box.first p.tec{ margin: 0; }

	/* regist
	------------------------------------------------------------ */
	.form_template dl{ border-bottom: none; }
	.form_template dl:last-child{ border-bottom: dashed 2px #ccc; }
	.form_template dt{
		padding: 20px;
		display: block;
		width: auto;
		background: url(../img/common/bg_formhead.gif) repeat;
		border-right: none;
	}
	.form_template dd{
		padding: 20px;
		display: block;
		width: auto;
	}
	.form_template dd input[type="text"],
	.form_template dd select, dd textarea{ width: 96%; padding: 5px 2%; }
	.form_template input[type="text"]{ height: 2em; }
	.form_template dd .innerlist_tel li input[type="text"]{ width: 15%; }
	.innerlist_name li {
		display: block;
		float: none;
		width: auto;
	}
	.innerlist_kana li {
		display: block;
		float: none;
		width: auto;
	}
	.innerlist_name li:first-child,
	.innerlist_kana li:first-child{ float: none; }

	/* thanks
	------------------------------------------------------------ */
	.thanks__info{
		display: block;
		padding: 20px;
		width: auto;
	}
	.thanks__info .pho{
		display: block;
		width: auto;
		max-width: 300px;
		margin: auto;
		padding: 0;
	}
	.thanks__info .pho img{ width: 100%; }
	.thanks__info .txt{
		display: block;
		width: auto;
		padding: 0;
		margin-top: 30px;
	}
}
